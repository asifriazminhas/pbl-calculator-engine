{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "quotemark": {
            "options": "single"
        },
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "no-eval": false,
        "no-console": false,
        "interface-name": false,
        "curly": [true, "ignore-same-line"],
        "trailing-comma": [true],
        "member-access": [true, "no-public"]
    },
    "rulesDirectory": []
}
