{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "quotemark": {
            "options": "single"
        },
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "no-eval": false,
        "no-console": false,
        "member-access": false,
        "interface-name": false,
        "member-ordering": [
            true,
            {
                "order": [
                    "fields-first",
                    "static-field",
                    "public-instance-method",
                    "private-instance-method"
                ]
            }
        ]
    },
    "rulesDirectory": []
}
