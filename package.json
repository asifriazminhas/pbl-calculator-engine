{
    "name": "@ottawamhealth/pbl-calculator-engine",
    "version": "1.19.1",
    "description": "The engine used to parse PMML files and evaluate algoirithms generated using PMML files",
    "main": "lib/index.js",
    "typings": "lib/index.d.ts",
    "scripts": {
        "build:dev": "npm run build-ts:watch",
        "build-umd": "webpack --config ./webpack/umd.config.js",
        "build-ts": "tsc",
        "build-ts:watch": "tsc -w",
        "build": "rm -rf ./lib && tsc --module es2015 --outDir lib-ts && babel ./lib-ts --out-dir ./lib --source-maps && rsync -a --exclude=\"*.js\" --include=\"*.d.ts|*.js.map\" ./lib-ts/ ./lib/ && rm -rf ./lib-ts && npm run build-umd",
        "prepublish": "npm run build",
        "lifetable-csv-to-json": "node ./lib/scripts/lifetable-csv-to-json.js",
        "test": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"CommonJS\"}' ts-node ./node_modules/tape/bin/tape ./src/test/**/*.ts | tap-spec"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Ottawa-mHealth/pbl-calculator-engine.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Ottawa-mHealth/pbl-calculator-engine/issues"
    },
    "homepage": "https://github.com/Ottawa-mHealth/pbl-calculator-engine#readme",
    "devDependencies": {
        "@babel/cli": "~7.7.0",
        "@babel/core": "~7.7.0",
        "@babel/preset-env": "~7.7.0",
        "@types/bluebird": "3.0.33",
        "@types/chai": "3.4.34",
        "@types/common-tags": "1.4.0",
        "@types/escodegen": "0.0.6",
        "@types/esprima": "^4.0.1",
        "@types/exceljs": "0.4.0",
        "@types/express": "4.0.36",
        "@types/lodash": "~4.14.52",
        "@types/memoize-one": "^3.1.1",
        "@types/mocha": "2.2.32",
        "@types/node": "12.12.x",
        "@types/source-map-support": "0.2.28",
        "@types/tape": "4.2.33",
        "@types/uuid": "3.4.6",
        "@types/xml2js": "0.0.28",
        "@types/xmlbuilder": "0.0.29",
        "@types/yargs": "6.6.0",
        "awesome-typescript-loader": "~3.5.0",
        "babel-loader": "6.2.8",
        "babel-plugin-lodash": "~3.3.4",
        "babel-polyfill": "6.23.0",
        "chai": "3.5.0",
        "prettier": "1.19.1",
        "tap-spec": "5.0.0",
        "tape": "4.11.0",
        "ts-node": "8.4.1",
        "tslib": "1.10.0",
        "tslint": "5.8.0",
        "typescript": "3.6.3",
        "webpack": "~4.27.0",
        "webpack-cli": "~3.1.0"
    },
    "dependencies": {
        "ast-types": "0.9.12",
        "bluebird": "3.4.6",
        "common-tags": "1.4.0",
        "core-decorators": "^0.20.0",
        "csv-parse": "4.8.2",
        "dependency-graph": "0.8.1",
        "escodegen": "1.8.1",
        "esprima": "^4.0.0",
        "lodash": "~4.17.12",
        "memoize-one": "^4.0.0",
        "moment": "^2.22.2",
        "source-map-support": "0.4.3",
        "utility-types": "2.1.0",
        "uuid": "3.3.3",
        "xml2js": "~0.4.0",
        "xmlbuilder": "^8.2.2",
        "yargs": "6.6.0"
    }
