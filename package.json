{
    "name": "@ottawamhealth/pbl-calculator-engine",
    "version": "1.1.1",
    "engines": {
        "node": "~7.x.x"
    },
    "description":
        "The engine used to parse PMML files and evaluate algoirithms generated using PMML files",
    "main": "lib/index.js",
    "typings": "lib/index.d.ts",
    "scripts": {
        "build-umd": "webpack --config ./webpack/umd.config.js",
        "build-ts": "tsc",
        "build-ts:watch": "tsc -w",
        "build":
            "rm -rf ./lib && rm -rf ./dist && npm run build-ts && npm run build-umd",
        "prepublish": "npm run build",
        "lifetable-csv-to-json": "node ./lib/scripts/lifetable-csv-to-json.js",
        "test":
            "tape lib/test/local-transformations-tape.js | node_modules/.bin/tap-spec"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Ottawa-mHealth/pbl-calculator-engine.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Ottawa-mHealth/pbl-calculator-engine/issues"
    },
    "homepage":
        "https://github.com/Ottawa-mHealth/pbl-calculator-engine#readme",
    "devDependencies": {
        "@types/bluebird": "3.0.33",
        "@types/chai": "3.4.34",
        "@types/csv-parse": "1.1.5",
        "@types/escodegen": "0.0.6",
        "@types/exceljs": "0.4.0",
        "@types/express": "4.0.36",
        "@types/lodash": "4.14.52",
        "@types/mocha": "2.2.32",
        "@types/node": "6.0.42",
        "@types/source-map-support": "0.2.28",
        "@types/tape": "4.2.30",
        "@types/xml2js": "0.0.28",
        "@types/xmlbuilder": "0.0.29",
        "@types/yargs": "6.6.0",
        "awesome-typescript-loader": "3.0.0-beta.9",
        "babel-core": "6.18.2",
        "babel-loader": "6.2.8",
        "babel-plugin-transform-object-assign": "6.22.0",
        "babel-polyfill": "6.23.0",
        "babel-preset-es2015": "6.18.0",
        "chai": "3.5.0",
        "mocha": "3.1.2",
        "prettier": "1.7.4",
        "tap-spec": "4.1.1",
        "tslib": "1.7.1",
        "tslint": "5.8.0",
        "typescript": "2.4.1",
        "webpack": "1.13.3"
    },
    "dependencies": {
        "@ottawamhealth/pbl-limesurvey-engine": "0.1.2",
        "ast-types": "0.9.12",
        "bluebird": "3.4.6",
        "csv-parse": "1.1.7",
        "escodegen": "1.8.1",
        "exceljs": "0.4.10",
        "lodash": "4.17.4",
        "moment": "2.17.1",
        "source-map-support": "0.4.3",
        "tape": "4.8.0",
        "xml2js": "0.4.17",
        "xmlbuilder": "8.2.2",
        "yargs": "6.6.0"
    }
}
